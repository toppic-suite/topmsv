{
  "compilerOptions": {
    "allowJs":true,
    "alwaysStrict": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "strictPropertyInitialization": true,
    "noImplicitThis": true,	
    "target": "es2018",
    "moduleResolution": "node",
    "lib": [ "es6", "dom","es2016" ],
    "skipLibCheck": true,
    "outDir": "./dist"
  },
  "include": ["./public/js/**/*", "./public/resources/**/*"]
}
